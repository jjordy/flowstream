{
  "name": "flowstream",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev --manual",
    "db:codegen": "kysely-codegen",
    "db:init": "npm run db:migrate && npm run db:codegen && npm run db:seed",
    "db:migrate": "tsx ./bin/index.ts migrate",
    "db:create:migration": "tsx ./bin/index.ts create-migration",
    "db:create:data-repository": "tsx ./bin/index.ts create-data-repository",
    "db:seed": "tsx ./bin/index.ts seed-db",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.3",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-toggle": "^1.0.3",
    "@remix-run/css-bundle": "^2.4.1",
    "@remix-run/node": "^2.4.1",
    "@remix-run/react": "^2.4.1",
    "@remix-run/serve": "^2.4.1",
    "better-sqlite3": "^9.2.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "dotenv": "^16.3.1",
    "isbot": "^3.6.8",
    "kysely": "^0.26.3",
    "kysely-codegen": "^0.11.0",
    "lucide-react": "^0.302.0",
    "nanoid": "^5.0.4",
    "next-themes": "^0.2.1",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "remix-themes": "^1.2.2",
    "sonner": "^1.3.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.4.1",
    "@types/better-sqlite3": "^7.6.8",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "autoprefixer": "^10.4.16",
    "commander": "^11.1.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "tsx": "4.7.0",
    "typescript": "^5.1.6",
    "vite": "^5.0.11"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "^pnpm@8.10.5"
}
